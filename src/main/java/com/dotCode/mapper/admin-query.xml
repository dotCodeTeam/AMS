<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="getEmpInfo">
        SELECT
                E.*
        FROM    EMPLOYEE E
        WHERE   E.EMP_NO = ?
    </entry>
    <entry key="getAllEmpInfo">
        SELECT *
        FROM    EMPLOYEE
    </entry>
    <entry key="getAtdInfo">
        SELECT
                A.*
        FROM    ATTENDANCE A
        WHERE   A.EMP_NO = ?
    </entry>
    <entry key="getAllAtdInfo">
        SELECT *
        FROM    ATTENDANCE
    </entry>
    <entry key="getVcntInfo">
        SELECT
                V.*
        FROM    VACANT V
        WHERE   V.EMP_NO = ?
    </entry>
    <entry key="getAllVcntInfo">
        SELECT *
        FROM    VACANT
    </entry>
    <entry key="updateVcntAcpt">
        UPDATE
                Vacant V
        SET
                V.ACCEPT_STATUS = ?
        WHERE
                V.EMP_NO = ?
    </entry>

    <entry key="updateAtdTotalDayCount">
        UPDATE
            ATTENDANCE A
        SET
            A.TOTAL_DAY_COUNT = ?
        WHERE
            A.EMP_NO = ?
    </entry>
    <entry key="updateAtdOntimeCount">
        UPDATE
            ATTENDANCE A
        SET
            A.ONTIME_COUNT = ?
        WHERE
            A.EMP_NO = ?
    </entry>
    <entry key="updateAtdLateCount">
        UPDATE
            ATTENDANCE A
        SET
            A.LATE_COUNT = ?
        WHERE
            A.EMP_NO = ?
    </entry>
    <entry key="updateAtdAbsentCount">
        UPDATE
            ATTENDANCE A
        SET
            A.ABSENT_COUNT = ?
        WHERE
            A.EMP_NO = ?
    </entry>
    <entry key="updateAtdTotalScore">
        UPDATE
            ATTENDANCE A
        SET
            A.TOTAL_SCORE = ?
        WHERE
            A.EMP_NO = ?
    </entry>


</properties>